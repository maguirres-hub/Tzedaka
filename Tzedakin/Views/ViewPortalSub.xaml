<?xml version="1.0" encoding="utf-8" ?>
<TabbedPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
            xmlns:local="clr-namespace:Tzedaka.Views" 
             x:Class="Tzedaka.Views.ViewPortalSub" x:Name="Portal_Subs" CurrentPageChanged="OnCurrentPageChanged">
    <TabbedPage.ToolbarItems >
        <ToolbarItem Text="Salir" Clicked="ToolbarItem_Clicked" />
    </TabbedPage.ToolbarItems>
    <local:ViewInicio IconImageSource="inicio"/>
    <local:ViewSubscriptores IconImageSource="bloques"/>
    <local:ViewCursos IconImageSource="cursos"/>
    <local:ViewTienda IconImageSource="tienda"/>
    <local:ViewBilletera IconImageSource="wallet"/>
    <local:ViewPerfil IconImageSource="perfil"/>
</TabbedPage>